@model dynamic

@{
    ViewBag.Title = "Runner";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Runner</h2>
<p>
    Lines in logfile: @ViewBag.Lines
</p>
@using (Html.BeginForm("Process", "Runner", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <p> Target url: <input type = "text" name = "target" value="http://alloy.local/" /></p>
    <input type="submit" name="Submit" id="Submit" value="Upload" />
}

<p><input type="button" value="run"/></p>